<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.roowoo.log.modules.count5.dao.Count5lDao">
    
	<sql id="count5lColumns">
		a.loadcount AS "loadcount",
		a.ip AS "ip"
	</sql>
	
	<sql id="count5lJoins">
	</sql>
    
	<select id="get" resultType="Count5l">
		SELECT 
			<include refid="count5lColumns"/>
		FROM count5l a
		<include refid="count5lJoins"/>
		WHERE a.id = #{id}
	</select>
	
	<select id="findList" resultType="Count5l">
		SELECT 
			<include refid="count5lColumns"/>
		FROM count5l a
		<include refid="count5lJoins"/>
		<where>
			
		</where>
		<choose>
			<when test="page !=null and page.orderBy != null and page.orderBy != ''">
				ORDER BY ${page.orderBy}
			</when>
			<otherwise>
			</otherwise>
		</choose>
	</select>
	
	<select id="findAllList" resultType="Count5l">
		SELECT 
			<include refid="count5lColumns"/>
		FROM count5l a
		<include refid="count5lJoins"/>
		<where>
			
		</where>		
		<choose>
			<when test="page !=null and page.orderBy != null and page.orderBy != ''">
				ORDER BY ${page.orderBy}
			</when>
			<otherwise>
			</otherwise>
		</choose>
	</select>
	
	<insert id="insert">
		INSERT INTO count5l(
			loadcount,
			ip
		) VALUES (
			#{loadcount},
			#{ip}
		)
	</insert>
	
	<update id="update">
		UPDATE count5l SET 	
			loadcount = #{loadcount},
			ip = #{ip}
		WHERE id = #{id}
	</update>
	
	<update id="delete">
		DELETE FROM count5l
		WHERE id = #{id}
	</update>
	
</mapper>