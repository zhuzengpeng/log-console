<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.roowoo.log.modules.bar.dao.LoginPwdNumDao">
    
	<sql id="loginPwdNumColumns">
		a.id AS "id",
		a.count AS "count",
		a.num AS "num"
	</sql>
	
	<sql id="loginPwdNumJoins">
	</sql>
    
	<select id="get" resultType="LoginPwdNum">
		SELECT 
			<include refid="loginPwdNumColumns"/>
		FROM login_pwd_num a
		<include refid="loginPwdNumJoins"/>
		WHERE a.id = #{id}
	</select>
	
	<select id="findList" resultType="LoginPwdNum">
		SELECT 
			<include refid="loginPwdNumColumns"/>
		FROM login_pwd_num a
		<include refid="loginPwdNumJoins"/>
		<where>
			
		</where>
		<choose>
			<when test="page !=null and page.orderBy != null and page.orderBy != ''">
				ORDER BY ${page.orderBy}
			</when>
			<otherwise>
			</otherwise>
		</choose>
	</select>
	
	<select id="findAllList" resultType="LoginPwdNum">
		SELECT 
			<include refid="loginPwdNumColumns"/>
		FROM login_pwd_num a
		<include refid="loginPwdNumJoins"/>
		<where>
			
		</where>
	</select>
	
	<insert id="insert">
		INSERT INTO login_pwd_num(
			id,
			count,
			num
		) VALUES (
			#{id},
			#{count},
			#{num}
		)
	</insert>
	
	<update id="update">
		UPDATE login_pwd_num SET 	
			count = #{count},
			num = #{num}
		WHERE id = #{id}
	</update>
	
	<update id="delete">
		DELETE FROM login_pwd_num
		WHERE id = #{id}
	</update>
	
</mapper>